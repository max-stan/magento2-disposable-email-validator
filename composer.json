    "keywords": ["magento2", "disposable", "block", "temporary", "email"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Stanislavskyi",
            "email": "stanislavskiymaksym@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "magento/framework": "*",
        "beeyev/disposable-email-filter-php": "^1.3"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "MaxStan\\DisposableEmailValidator\\": ""
        }
    }
}
